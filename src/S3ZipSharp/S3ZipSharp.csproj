<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Title>S3ZipSharp</Title>
    <PackageId>S3ZipSharp</PackageId>
    <Description>A tool to retrieve and zip objects from a s3 bucket and zip them up, uploading the zip file back into s3.</Description>
    <Copyright>MIT</Copyright>
    <RepositoryUrl>https://github.com/SeanoNET/S3ZipSharp</RepositoryUrl>
    <PackageTags>aws; s3; zip;</PackageTags>
    <Authors>Sean O.</Authors>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <PackageProjectUrl>https://github.com/SeanoNET/S3ZipSharp</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <Version>0.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.5.1.4" />
    <PackageReference Include="DotNetZip" Version="1.13.8" />
  </ItemGroup>

</Project>
